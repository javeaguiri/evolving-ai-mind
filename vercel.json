{
  "builds": [
    {
      "src": "src/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/slackbot",         
      "dest": "/src/api/slackbot/index.js"
    },
    {
      "src": "/api/slackbot/(.*)", 
      "dest": "/src/api/slackbot/index.js"
    },
    {
      "src": "/api/process/(.*)",
      "dest": "/src/api/process/index.js"
    },
    {
      "src": "/api/service/(.*)",
      "dest": "/src/api/service/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/api/404.js"
    }
  ]
}
